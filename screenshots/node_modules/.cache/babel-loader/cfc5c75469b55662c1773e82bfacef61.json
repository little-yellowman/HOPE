\t\tthis.imgUrl = res\r\n\t\t\tthis.total = res.length\r\n\t\t\t// console.log(res)\r\n\t\t}\r\n\t}\r\n  }\r\n</script>\n\n<style scoped=\"scoped\">\r\n\t.imgCon{\r\n\t\twidth: 90px;\r\n\t\theight: 175px;\r\n\t\t/* background-color: red; */\r\n\t\t/* margin-right: 11px;\r\n\t\tmargin-bottom: 32px; */\r\n\t\tmargin: 20px 10px;\r\n\t\tfloat: left;\r\n\t\tbox-shadow: 0 0 10px #909399;\r\n\t}\r\n\t.imgName{\r\n\t\twidth: 100px;\r\n\t\tfont-size: 12px;\r\n\t\t/* background-color: green; */\r\n\t\tcolor: #303133;\r\n\t\twhite-space: normal;\r\n\t\ttext-overflow: ellipsis;\r\n\t\toverflow: hidden;\r\n\t\tline-height: 12px;\r\n\t\theight: 24px;\r\n\t\t\r\n\t\tmargin-top: 5px;\r\n\t}\r\n\t.pagination{\r\n\t\tposition: absolute;\r\n\t\tbottom: 5px;\r\n\t\twidth: 80%;\r\n\t\tmargin-left: 10%;\r\n\t}\r\n\t.dowload{\r\n\t\tposition: absolute;\r\n\t\ttop: 18px;\r\n\t\tright: 5px;\r\n\t\tfloat: right;\r\n\t}\n</style>\n"],"sourceRoot":"src/components/admin"}]}